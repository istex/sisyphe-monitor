{
  "name": "sisyphe-monitor",
  "version": "2.2.1",
  "main": "index.js",
  "dependencies": {
    "bluebird": "^3.5.0",
    "bower": "^1.8.2",
    "localStorage": "^1.0.3",
    "redis": "^2.8.0",
    "request": "^2.81.0"
  },
  "scripts": {
    "postinstall": "bower install",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=all --out=build",
    "package-mac": "electron-packager . --icon=icon-sisyphe.icns --overwrite --platform=darwin --arch=all --out=build",
    "package-windows": "electron-packager . --icon=icon-sisyphe.ico --overwrite --platform=win32 --arch=all --out=build"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-packager": "^9.1.0"
  }
}
