{
  "name": "sisyphe-monitor",
  "version": "3.9.0",
  "main": "index.js",
  "dependencies": {
    "angular-ui-sortable": "^0.17.2",
    "bluebird": "^3.5.1",
    "bower": "^1.8.2",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.11.0",
    "electron-context-menu": "^0.9.1",
    "electron-forge": "^4.1.5",
    "electron-oauth2": "^3.0.0",
    "jquery-ui": "^1.12.1",
    "lerna": "^2.5.1",
    "localStorage": "^1.0.3",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.2",
    "sisyphe": "git+https://github.com/istex/sisyphe.git#electrontest"
  },
  "scripts": {
    "electron-rebuild-win": "electron-rebuild node_modules/sisyphe/src/worker/filetype && electron-rebuild node_modules/sisyphe/src/worker/xml &&electron-rebuild node_modules/sisyphe/src/worker/pdf && electron-rebuild node_modules/sisyphe/src/worker/nb && electron-rebuild node_modules/sisyphe/src/worker/multicat && electron-rebuild node_modules/sisyphe/src/worker/teeft && electron-rebuild node_modules/sisyphe/src/worker/skeeft && electron-rebuild node_modules/sisyphe/src/worker/lang",
    "electron-rebuild-unix": "electron-rebuild node_modules/sisyphe/",
    "electron": "electron .",
    "postinstall": "bower install",
    "postinstall-unix": "cd node_modules/sisyphe && npm run postinstall-unix",
    "postinstall-win": "cd node_modules/sisyphe && npm run postinstall-win",
    "orchestra": "node script/orchestra.js",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=all --out=build",
    "package-mac": "electron-packager . --icon=public/img/icon.icns --overwrite --platform=darwin --arch=all --out=build",
    "package-windows": "electron-packager . --icon=public/img/icon.ico --overwrite --platform=win32 --arch=all --out=build"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^9.1.0",
    "electron-prebuilt-compile": "1.7.9",
    "electron-publish": "^19.46.5",
    "electron-rebuild": "^1.6.0",
    "electron-builder": "^19.46.9"
  }
}
